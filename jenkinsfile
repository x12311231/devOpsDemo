pipeline {
    agent any

    stages {
        stage('拉取代码') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '${tag}']], extensions: [], userRemoteConfigs: [[credentialsId: '957944e5-1945-412b-8a6a-dbdefb41fdfa', url: 'http://web:8929/gitlab-instance-8982e4fb/test.git']]])
                echo '拉取代码'
            }
        }
        stage('sonarqube') {
            steps {
                echo '质量检测1'
            }
        }
        stage('build') {
            steps {
                echo 'build project'
            }
        }
        stage('push') {
            steps {
                echo 'push project to repo'
            }
        }
        stage('notice') {
            steps {
                echo 'notice target'
            }
        }
    }
}
